  "description": "",
  "main": "index.js",
  "type": "commonjs",
   "watch": {
    "start":{
      "patterns": ["src"],
      "extensions": "ts,ejs",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500,
      "runOnChangeOnly": false
    }
